generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model posts {
  id          Int    @id @default(autoincrement())
  username    String @db.VarChar(255)
  blogtitle   String @db.VarChar(255)
  blogcontent String
}
